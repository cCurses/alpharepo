# false

_parse_parameters "$@"

if [[ $_BINARY_WMCTRL == false ]] \
&& ! $_BOOLEAN_LIST
then
  echo "binary \"wmctrl\" not found, faked it with \"false\" instead"
fi

#- start here -#

