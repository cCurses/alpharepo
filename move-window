  # sed

  # builtins:
  # echo

  # NET:
  # - _NET_WM_PID, CARDINAL/32

  # "_NET_WM_PID(CARDINAL) = 1454"
  # "1454"

  # NOTE:
  # - Openbox does not currently kill processes.

  _NET_WM_PID_CARDINAL=$(echo "$_OUTPUT_XPROP_ID" | sed --expression='/^_NET_WM_PID(CARDINAL) = /!d ; /.* = / s///g')

