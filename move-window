# - the following are currently set in "function _sub_function" of
#   "function _get_net_workarea_cardinal"
# - all of this will be done in "function _workarea_borders_calculate" ASAP
#   (done. might revert) (reverted)
# - so.. _WORKAREA_BORDER_ has to stay

#  if $_BOOLEAN_OVERALL
#  then
# overall
#    _WORKAREA_BORDER_L=0
#    _WORKAREA_BORDER_T=0
#    _WORKAREA_BORDER_R=0
#    _WORKAREA_BORDER_B=0
#  else
#    # - I suspect these calculations are incorrect. aka: they seem to work
#    #   purely by accident
#
# desktop
#    # - $((expression))
#    _WORKAREA_BORDER_L=$((                                               $_NET_WORKAREA_X )) # 0               = 0
#    # - $((expression))
#    _WORKAREA_BORDER_T=$((                                               $_NET_WORKAREA_Y )) # 0               = 0
#    # - $((expression))
#    _WORKAREA_BORDER_R=$(( $_NET_DESKTOP_GEOMETRY_W - $_NET_WORKAREA_W - $_NET_WORKAREA_X )) # 1920 - 1852 - 0 = 68
#    # - $((expression))
#    _WORKAREA_BORDER_B=$(( $_NET_DESKTOP_GEOMETRY_H - $_NET_WORKAREA_H - $_NET_WORKAREA_Y )) # 1080 - 1056 - 0 = 24
#  fi

# IDEA: (vague as it is)
# - allow for fractional coordinates. ie: (X,Y) of (2/5,3/7)
# - test with fractions that are multiples of 3 since I already have the
#   calculations available for comparison
# - numerator & denominator expressed as $_NUMERATOR & $_DENOMINATOR
# - something about each " - 1" &/or " + 1"
# - start counting from "1", so top/left corner would be "1/?,1/?"
# - calculations use 0 to 8, options should be expressed as 1 to 9
# - use ( $_NUMERATOR - 1 ) & ( $_DENOMINATOR - 1 ) in the calculations?
# - lots of trapping will be required:
#+ incomplete/wrong, but on the right track
# > no negative numbers
# > actually, no numbers lower than "0"
# > denominator cannot be lower than 1
# > $_NUMERATOR -le $_DENOMINATOR
#-

# TODO:
# - there's something being calculated incorrectly for AA when the dock is
#   upper/left
#   (BA > EA appear to be fine)
# > this needs verification. a large number of changes have taken place since
#   the above note was written. ditto for the note immediately below

# NOTE:
# - order of operations *does* matter due to using bash's "$(())". it uses
#   whole numbers in its calculations (the whole "round down" vs "round up"
#   scenario comes into play)
# > " / 8 * 6 " vs " * 6 / 8 " (using the latter form for correctness)

# NOTE:
# - tested on the following display sizes. all the raw numbers below are from
#   the largest display
# > 1920,1080
# > 1366,768
# > 1024,768

# 9x9 coordinates
# desktop ##########################################
# X =    0  193  387  581  775  968 1162 1356 1550 #
# Y =    0  103  207  310  414  517  621  724  828 #
# overall ##########################################
# X =    0  202  404  606  809 1011 1213 1415 1618 #
# Y =    0  106  213  319  426  532  639  745  852 #
####################################################

# 5x5 coordinates
# desktop ##########################################
# X =    0       387       775      1162      1550 #
# Y =    0       207       414       621       828 #
# overall ##########################################
# X =    0       404       809      1213      1618 #
# Y =    0       213       426       639       852 #
####################################################

# 3x3 coordinates
# desktop ##########################################
# X =    0                 775                1550 #
# Y =    0                 414                 828 #
# overall ##########################################
# X =    0                 809                1618 #
# Y =    0                 426                 852 #
####################################################

  case $_OPTION_WINDOW_COORDINATES in
    ( A1 | AA | ul ) # Upper/Left (shared 1/4)
# desktop
#                 new coordinates (X,Y): "0,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,0,0,-1,-1"
# overall
#                 new coordinates (X,Y): "0,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,0,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 0 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 0 / 8 )) # = 0
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 0 / 8 )) # = 0
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( A2 )
# desktop
#                 new coordinates (X,Y): "193,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,193,0,-1,-1"
# overall
#                 new coordinates (X,Y): "202,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,202,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 1 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 1 / 8 )) # = 193
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 1 / 8 )) # = 202
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( A3 | AB )
# desktop
#                 new coordinates (X,Y): "387,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,387,0,-1,-1"
# overall
#                 new coordinates (X,Y): "404,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,404,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 2 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 2 / 8 )) # = 387
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 2 / 8 )) # = 404
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( A4 )
# desktop
#                 new coordinates (X,Y): "581,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,581,0,-1,-1"
# overall
#                 new coordinates (X,Y): "606,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,606,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 3 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 3 / 8 )) # = 581
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 3 / 8 )) # = 606
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( A5 | AC | uc ) # Upper/Center
# desktop
#                 new coordinates (X,Y): "775,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,775,0,-1,-1"
# overall
#                 new coordinates (X,Y): "809,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,809,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 4 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 4 / 8 )) # = 775
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 4 / 8 )) # = 809
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( A6 )
# desktop
#                 new coordinates (X,Y): "968,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,968,0,-1,-1"
# overall
#                 new coordinates (X,Y): "1011,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,1011,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 5 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 5 / 8 )) # = 968
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 5 / 8 )) # = 1011
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( A7 | AD )
# desktop
#                 new coordinates (X,Y): "1162,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,1162,0,-1,-1"
# overall
#                 new coordinates (X,Y): "1213,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,1213,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 6 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 6 / 8 )) # = 1162
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 6 / 8 )) # = 1213
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( A8 )
# desktop
#                 new coordinates (X,Y): "1356,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,1356,0,-1,-1"
# overall
#                 new coordinates (X,Y): "1415,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,1415,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 7 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 7 / 8 )) # = 1356
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 7 / 8 )) # = 1415
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( A9 | AE | ur ) # Upper/Right (shared 2/4)
# desktop
#                 new coordinates (X,Y): "1550,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,1550,0,-1,-1"
# overall
#                 new coordinates (X,Y): "1618,0"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,1618,0,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 8 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 8 / 8 )) # = 1550
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 8 / 8 )) # = 1618
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 0 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 0 / 8 )) # = 0
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 0 / 8 )) # = 0
    ;;
    ( B1 )
# desktop
#                 new coordinates (X,Y): "0,103"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,0,103,-1,-1"
# overall
#                 new coordinates (X,Y): "0,106"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,0,106,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 0 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 0 / 8 )) # = 0
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 0 / 8 )) # = 0
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 1 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 1 / 8 )) # = 103
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 1 / 8 )) # = 106
    ;;
    ( B2 )
# desktop
#                 new coordinates (X,Y): "193,103"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,193,103,-1,-1"
# overall
#                 new coordinates (X,Y): "202,106"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,202,106,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 1 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 1 / 8 )) # = 193
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 1 / 8 )) # = 202
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 1 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 1 / 8 )) # = 103
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 1 / 8 )) # = 106
    ;;
    ( B3 )
# desktop
#                 new coordinates (X,Y): "387,103"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,387,103,-1,-1"
# overall
#                 new coordinates (X,Y): "404,106"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,404,106,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 2 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 2 / 8 )) # = 387
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 2 / 8 )) # = 404
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 1 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 1 / 8 )) # = 103
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 1 / 8 )) # = 106
    ;;
    ( B4 )
# desktop
#                 new coordinates (X,Y): "581,103"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,581,103,-1,-1"
# overall
#                 new coordinates (X,Y): "606,106"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,606,106,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 3 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 3 / 8 )) # = 581
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 3 / 8 )) # = 606
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 1 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 1 / 8 )) # = 103
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 1 / 8 )) # = 106
    ;;
    ( B5 )
# desktop
#                 new coordinates (X,Y): "775,103"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,775,103,-1,-1"
# overall
#                 new coordinates (X,Y): "809,106"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,809,106,-1,-1"
      # - $((expression))
      _COORDINATE_X=$(( ( $_NET_DESKTOP_GEOMETRY_W + $_WORKAREA_BORDER_L - $_WINDOW_DECORATED_W - $_WORKAREA_BORDER_R ) * 4 / 8 ))
#                   $(( ( 1920                     + 0                   - 302                  - 68                  ) * 4 / 8 )) # = 775
#                   $(( ( 1920                     + 0                   - 302                  - 0                   ) * 4 / 8 )) # = 809
      # - $((expression))
      _COORDINATE_Y=$(( ( $_NET_DESKTOP_GEOMETRY_H + $_WORKAREA_BORDER_T - $_WINDOW_DECORATED_H - $_WORKAREA_BORDER_B ) * 1 / 8 ))
#                   $(( ( 1080                     + 0                   - 228                  - 24                  ) * 1 / 8 )) # = 103
#                   $(( ( 1080                     + 0                   - 228                  - 0                   ) * 1 / 8 )) # = 106
    ;;
    ( B6 )
# desktop
#                 new coordinates (X,Y): "968,103"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,968,103,-1,-1"
# overall
#                 new coordinates (X,Y): "1011,106"
#                 wmctrl command (move): "wmctrl -i -r 0x1600099 -e 0,1011,106,-1,-1"
